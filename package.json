{
  "name": "team-preview",
  "version": "1.2.0",
  "description": "Pokémon team preview image generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npx tsc -p ./ && node src/debug.mjs",
    "build": "npx tsc -p ./",
    "postversion": "git push && git push --tags",
    "prepack": "npx tsc -p ./"
  },
  "files": [
    "dist/**/*",
    "data/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrifcastr/team-preview.git"
  },
  "keywords": [
    "Pokémon",
    "team",
    "preview",
    "generator",
    "showdown",
    "summary"
  ],
  "author": "castdrian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrifcastr/team-preview/issues"
  },
  "homepage": "https://github.com/adrifcastr/team-preview#readme",
  "eslintConfig": {
	"extends": ["standard-typescript-prettier"],
	"parserOptions": { "project": "./tsconfig.json" }
  },
  "prettier": "eslint-config-standard-typescript-prettier/prettier",
  "dependencies": {
    "@pkmn/data": "^0.4.6",
    "@pkmn/dex": "^0.4.6",
    "@pkmn/img": "^0.1.7",
    "@pkmn/sets": "^2.0.0",
    "gif-frames": "^1.0.1",
    "gifencoder": "^2.0.1",
    "skia-canvas": "^0.9.21",
    "tslib": "^2.2.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/gifencoder": "^2.0.1",
    "@types/node": "^17.0.21",
    "@types/skia-canvas": "^0.9.0",
    "eslint-config-standard-typescript-prettier": "^6.0.0"
  }
}
